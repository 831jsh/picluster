# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
# and trailing whitespace removed
[*]
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.{js,css,json,yml}]
charset = utf-8
indent_style = space
indent_size = 2

[*.html]
indent_style = tab
